{
    "author": "Duane Sibilly <duane@sibilly.com> (https://github.com/dsibilly/)",
    "babel": {
        "comments": false,
        "plugins": [
            "transform-line",
            "transform-runtime"
        ],
        "presets": [
            "env"
        ],
        "sourceMaps": "inline"
    },
    "bin": {
        "dice-tower": "bin/dice-tower"
    },
    "bugs": {
        "url": "https://github.com/dsibilly/dice-tower/issues"
    },
    "contributors": [
        {
            "email": "troygoode@gmail.com",
            "name": "Troy Goode",
            "web": "https://github.com/troygoode"
        },
        {
            "email": "cajahnke@users.noreply.github.com",
            "name": "cajahnke",
            "web": "https://github.com/cajahnke/"
        },
        {
            "email": "mfunk86@gmail.com",
            "name": "Mark Funk"
        },
        {
            "email": "duane@sibilly.com",
            "name": "Duane Sibilly",
            "web": "https://github.com/dsibilly/"
        }
    ],
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "isotropic-error": "^0.6.0",
        "isotropic-make": "^0.6.0"
    },
    "description": "A Node.js module for rolling dice and adding modifiers, e.g.: 3d6+2. Forked from node-roll.",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-istanbul": "^0.12.2",
        "babel-plugin-transform-line": "^0.4.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "chai": "^4.1.2",
        "depcheck": "^0.6.9",
        "eslint": "^4.19.1",
        "eslint-config-isotropic": "^0.6.0",
        "mocha": "^5.1.0",
        "nsp": "^3.2.1"
    },
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "mocha": true,
            "node": true
        },
        "extends": "isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2017,
            "sourceType": "module"
        },
        "root": true
    },
    "homepage": "https://github.com/dsibilly/dice-tower/",
    "keywords": [
        "dice",
        "dice tower",
        "games",
        "random",
        "role playing",
        "roll",
        "rpg"
    ],
    "license": "MIT",
    "main": "lib/DiceTower.js",
    "name": "dice-tower",
    "repository": {
        "type": "git",
        "url": "git://github.com/dsibilly/dice-tower.git"
    },
    "scripts": {
        "build": "BABEL_ENV=build babel js -d lib",
        "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha --report lcovonly -- -R spec",
        "lint": "node_modules/.bin/eslint bin js",
        "posttest": "[ -z \"$npm_config_coverage\" ] || babel-istanbul check-coverage --branches 80 --functions 100 --lines 100 --statements 100",
        "prepare": "npm run build",
        "prepublishOnly": "npm run securityCheck && npm test --coverage",
        "pretest": "npm run lint",
        "rebuild": "rm -rf ./node_modules && rm package-lock.json && npm install",
        "securityCheck": "nsp check",
        "test": "npm run build && babel-node ./node_modules/.bin/babel-istanbul test _mocha"
    },
    "version": "1.0.0"
}

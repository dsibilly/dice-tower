{
    "author": "Duane Sibilly <duane@sibilly.com> (https://github.com/dsibilly/)",
    "babel": {
        "env": {
            "test": {
                "plugins": [
                    "istanbul"
                ]
            }
        },
        "plugins": [
            "transform-line"
        ],
        "presets": [
            "@babel/preset-env"
        ]
    },
    "bin": {
        "dice-tower": "bin/dice-tower"
    },
    "bugs": {
        "url": "https://github.com/dsibilly/dice-tower/issues"
    },
    "contributors": [
        {
            "email": "troygoode@gmail.com",
            "name": "Troy Goode",
            "web": "https://github.com/troygoode"
        },
        {
            "email": "cajahnke@users.noreply.github.com",
            "name": "cajahnke",
            "web": "https://github.com/cajahnke/"
        },
        {
            "email": "mfunk86@gmail.com",
            "name": "Mark Funk"
        },
        {
            "email": "duane@sibilly.com",
            "name": "Duane Sibilly",
            "web": "https://github.com/dsibilly/"
        }
    ],
    "dependencies": {
        "isotropic-error": "~0.7.0",
        "isotropic-make": "~0.7.0",
        "presage": "~0.2.0"
    },
    "description": "A Node.js module for rolling dice and adding modifiers, e.g.: 3d6+2. Forked from node-roll.",
    "devDependencies": {
        "@babel/cli": "~7.1.5",
        "@babel/core": "~7.1.6",
        "@babel/node": "~7.0.0",
        "@babel/preset-env": "~7.1.6",
        "@babel/register": "~7.0.0",
        "@babel/runtime": "~7.1.5",
        "babel-plugin-istanbul": "~5.1.0",
        "babel-plugin-transform-line": "~0.5.0",
        "chai": "~4.2.0",
        "coveralls": "~3.0.2",
        "cross-env": "~5.2.0",
        "depcheck": "~0.6.11",
        "eslint": "~5.9.0",
        "eslint-plugin-isotropic": "~0.2.0",
        "mocha": "~5.2.0",
        "nyc": "~13.1.0",
        "pre-commit": "~1.2.2"
    },
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "mocha": true,
            "node": true
        },
        "extends": "plugin:isotropic/isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "plugins": [
            "isotropic"
        ],
        "root": true,
        "rules": {
            "sort-imports": 0
        }
    },
    "homepage": "https://github.com/dsibilly/dice-tower/",
    "keywords": [
        "dice",
        "dice tower",
        "games",
        "random",
        "role playing",
        "roll",
        "rpg"
    ],
    "license": "MIT",
    "main": "lib/DiceTower.js",
    "name": "@dsibilly/dice-tower",
    "nyc": {
        "all": true,
        "cache": true,
        "check-coverage": true,
        "include": [
            "js/*.js",
            "js/*/*.js"
        ],
        "exclude": [
            "lib/*"
        ],
        "instrument": false,
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "require": [
            "@babel/register"
        ],
        "sourceMap": false
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/dsibilly/dice-tower.git"
    },
    "scripts": {
        "build": "BABEL_ENV=build babel js -d lib",
        "coverage": "nyc check-coverage",
        "lint": "node_modules/.bin/eslint bin/* js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || nyc check-coverage",
        "prepare": "npm run build",
        "prepublishOnly": "npm audit && nyc check-coverage",
        "pretest": "npm run lint",
        "rebuild": "rm -rf ./node_modules && rm package-lock.json && npm install",
        "test": "cross-env BABEL_ENV=test nyc mocha"
    },
    "version": "1.1.0"
}
